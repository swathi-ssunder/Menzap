<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diynet.menzap" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MenzapTheme" >
        <activity
            android:name="com.diynet.menzap.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.diynet.menzap.activity.IconTabsActivity"
            android:label="@string/title_activity_icon_tabs"
            android:parentActivityName="com.diynet.menzap.activity.MainActivity" >
        </activity>
    </application>

</manifest>
